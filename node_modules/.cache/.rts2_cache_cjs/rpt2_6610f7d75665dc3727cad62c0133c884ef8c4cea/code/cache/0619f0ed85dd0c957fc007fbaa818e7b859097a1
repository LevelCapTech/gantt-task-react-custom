{"code":"import React from \"react\";\r\nexport const Arrow = ({ taskFrom, taskTo, rowHeight, taskHeight, arrowIndent, rtl, }) => {\r\n    let path;\r\n    let trianglePoints;\r\n    if (rtl) {\r\n        [path, trianglePoints] = drownPathAndTriangleRTL(taskFrom, taskTo, rowHeight, taskHeight, arrowIndent);\r\n    }\r\n    else {\r\n        [path, trianglePoints] = drownPathAndTriangle(taskFrom, taskTo, rowHeight, taskHeight, arrowIndent);\r\n    }\r\n    return (React.createElement(\"g\", { className: \"arrow\" },\r\n        React.createElement(\"path\", { strokeWidth: \"1.5\", d: path, fill: \"none\" }),\r\n        React.createElement(\"polygon\", { points: trianglePoints })));\r\n};\r\nconst drownPathAndTriangle = (taskFrom, taskTo, rowHeight, taskHeight, arrowIndent) => {\r\n    const indexCompare = taskFrom.index > taskTo.index ? -1 : 1;\r\n    const taskToEndPosition = taskTo.y + taskHeight / 2;\r\n    const taskFromEndPosition = taskFrom.x2 + arrowIndent * 2;\r\n    const taskFromHorizontalOffsetValue = taskFromEndPosition < taskTo.x1 ? \"\" : `H ${taskTo.x1 - arrowIndent}`;\r\n    const taskToHorizontalOffsetValue = taskFromEndPosition > taskTo.x1\r\n        ? arrowIndent\r\n        : taskTo.x1 - taskFrom.x2 - arrowIndent;\r\n    const path = `M ${taskFrom.x2} ${taskFrom.y + taskHeight / 2} \n  h ${arrowIndent} \n  v ${(indexCompare * rowHeight) / 2} \n  ${taskFromHorizontalOffsetValue}\n  V ${taskToEndPosition} \n  h ${taskToHorizontalOffsetValue}`;\r\n    const trianglePoints = `${taskTo.x1},${taskToEndPosition} \n  ${taskTo.x1 - 5},${taskToEndPosition - 5} \n  ${taskTo.x1 - 5},${taskToEndPosition + 5}`;\r\n    return [path, trianglePoints];\r\n};\r\nconst drownPathAndTriangleRTL = (taskFrom, taskTo, rowHeight, taskHeight, arrowIndent) => {\r\n    const indexCompare = taskFrom.index > taskTo.index ? -1 : 1;\r\n    const taskToEndPosition = taskTo.y + taskHeight / 2;\r\n    const taskFromEndPosition = taskFrom.x1 - arrowIndent * 2;\r\n    const taskFromHorizontalOffsetValue = taskFromEndPosition > taskTo.x2 ? \"\" : `H ${taskTo.x2 + arrowIndent}`;\r\n    const taskToHorizontalOffsetValue = taskFromEndPosition < taskTo.x2\r\n        ? -arrowIndent\r\n        : taskTo.x2 - taskFrom.x1 + arrowIndent;\r\n    const path = `M ${taskFrom.x1} ${taskFrom.y + taskHeight / 2} \n  h ${-arrowIndent} \n  v ${(indexCompare * rowHeight) / 2} \n  ${taskFromHorizontalOffsetValue}\n  V ${taskToEndPosition} \n  h ${taskToHorizontalOffsetValue}`;\r\n    const trianglePoints = `${taskTo.x2},${taskToEndPosition} \n  ${taskTo.x2 + 5},${taskToEndPosition + 5} \n  ${taskTo.x2 + 5},${taskToEndPosition - 5}`;\r\n    return [path, trianglePoints];\r\n};\r\n//# sourceMappingURL=arrow.js.map","references":["/home/runner/work/gantt-task-react-custom/gantt-task-react-custom/node_modules/@types/react/ts5.0/index.d.ts","/home/runner/work/gantt-task-react-custom/gantt-task-react-custom/src/types/bar-task.ts"],"map":"{\"version\":3,\"file\":\"arrow.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/other/arrow.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,MAAM,OAAO,CAAC;AAW1B,MAAM,CAAC,MAAM,KAAK,GAAyB,CAAC,EAC1C,QAAQ,EACR,MAAM,EACN,SAAS,EACT,UAAU,EACV,WAAW,EACX,GAAG,GACJ,EAAE,EAAE;IACH,IAAI,IAAY,CAAC;IACjB,IAAI,cAAsB,CAAC;IAC3B,IAAI,GAAG,EAAE;QACP,CAAC,IAAI,EAAE,cAAc,CAAC,GAAG,uBAAuB,CAC9C,QAAQ,EACR,MAAM,EACN,SAAS,EACT,UAAU,EACV,WAAW,CACZ,CAAC;KACH;SAAM;QACL,CAAC,IAAI,EAAE,cAAc,CAAC,GAAG,oBAAoB,CAC3C,QAAQ,EACR,MAAM,EACN,SAAS,EACT,UAAU,EACV,WAAW,CACZ,CAAC;KACH;IAED,OAAO,CACL,2BAAG,SAAS,EAAC,OAAO;QAClB,8BAAM,WAAW,EAAC,KAAK,EAAC,CAAC,EAAE,IAAI,EAAE,IAAI,EAAC,MAAM,GAAG;QAC/C,iCAAS,MAAM,EAAE,cAAc,GAAI,CACjC,CACL,CAAC;AACJ,CAAC,CAAC;AAEF,MAAM,oBAAoB,GAAG,CAC3B,QAAiB,EACjB,MAAe,EACf,SAAiB,EACjB,UAAkB,EAClB,WAAmB,EACnB,EAAE;IACF,MAAM,YAAY,GAAG,QAAQ,CAAC,KAAK,GAAG,MAAM,CAAC,KAAK,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAC5D,MAAM,iBAAiB,GAAG,MAAM,CAAC,CAAC,GAAG,UAAU,GAAG,CAAC,CAAC;IACpD,MAAM,mBAAmB,GAAG,QAAQ,CAAC,EAAE,GAAG,WAAW,